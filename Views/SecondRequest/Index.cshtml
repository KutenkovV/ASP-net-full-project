@model IEnumerable<Accounting_of_Goods_ver_2.Models.outputDok_second_Result>

@{
    ViewBag.Title = "Второй запрос";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken();
    <div class="form-horizontal">
        <div class="text-center">
            <h2>Второй запрос</h2>
            <p>
                Для заданной торговой точки выдать список запрашиваемых товаров
                с указанием их количества на каждом складе, упорядоченный по
                наименованиям товаром и по номерам складов.
            </p>
        </div>

            @Html.ValidationSummary(true, "", new { @class = "alert alert-success" })
            <div class="form-group">
                <div class="col-md-10">
                    @Html.DropDownList("Retail_outlets_Addressr", null, htmlAttributes: new { @class = "form-control" })
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-10">
                    <input type="submit" value="Найти" class="btn btn-outline-success" />
                </div>
            </div>
    </div>
}